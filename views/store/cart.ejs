    <section id="cart_items">
      <div class="container">
        <div class="breadcrumbs">
          <ol class="breadcrumb">
            <li><a href="/store">Home</a></li>
            <li class="active">Shopping Cart</li>
          </ol>
        </div>

        <div class="table-responsive cart_info">
          <table class="table table-condensed">
            <thead>
              <tr class="cart_menu">
                <td class="image">Item</td>
                <td class="description"></td>
                <td class="price">Price</td>
                <td class="quantity">Quantity</td>
                <td class="total">Total</td>
                <td></td>
                <td></td>
              </tr>
            </thead>
            <tbody>
              <% for(i=0; i< info.length; i++){ %>
              <tr>
                <td class="cart_product">
                  <a href=""
                    ><img
                      src="http://localhost:3000/abc/uploads/<%= info[i].product_image %>"
                      height="140px"
                      width="120px"
                      alt=""
                  /></a>
                </td>
                <td class="cart_description">
                  <h4>
                    <a href="/product/<%= info[i].product_image %>"
                      ><%= info[i].product_name %></a
                    >
                  </h4>
                  <p>Brand Name: <%= info[i].product_brand %></p>
                </td>
                <td class="cart_price">
                  <p>$<%= info[i].product_price %></p>
                </td>
                <form action="/cart/order/<%= info[i].cart_id %>" method="POST">
                  <td
                    class="cart_quantity"
                    style="position: relative; bottom: 5px"
                  >
                    <div class="cart_quantity_button">
                      <input
                        type="hidden"
                        name="cu_id"
                        value="<%= customerID[0].id %>"
                      />
                      <input
                        type="hidden"
                        name="id"
                        value="<%= info[i].id %>"
                      />
                      <input
                        class="cart_quantity_input"
                        type="text"
                        value="<%= info[i].quantity%>"
                        disabled="disabled"
                        size="2"
                      />
                      <input
                        class="cart_quantity_input"
                        type="hidden"
                        name="quantity"
                        value="<%= info[i].quantity%>"
                      />
                      <!-- <a class="cart_quantity_down" href=""> - </a> -->
                    </div>
                  </td>
                  <td class="cart_total">
                    <p class="cart_total_price">
                      $<%= info[i].product_price*info[i].quantity %>
                    </p>
                    <input
                      type="hidden"
                      name="price"
                      value="<%= info[i].product_price*info[i].quantity %>"
                    />
                  </td>

                  <td style="position: relative; bottom: 5px">
                    <div class="cart_quantity_button">
                      <!-- <a href="" style="background-color: #faab3f; color: white; width:80px; font-size: 16px; padding: 5px; border: none;"">order</a> -->
                      <input
                        type="Submit"
                        value="order"
                        style="
                          background-color: #faab3f;
                          color: white;
                          width: 80px;
                          font-size: 16px;
                          padding: 5px;
                          border: none;
                        "
                      />
                    </div>
                  </td>
                </form>
                <td class="cart_delete">
                  <a
                    class="cart_quantity_delete"
                    href="/cart/delete/<%= info[i].cart_id %>"
                    ><i class="fa fa-times"></i
                  ></a>
                </td>
              </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
    </section>
